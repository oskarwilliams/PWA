{
  "name": "progressivewebapp",
  "version": "1.0.0",
  "description": "ProgressiveWebApp",
  "scripts": {
    "watch-frontend": "nodemon --watch ./frontend --ignore ./frontend/build --exec \"npm run build --prefix ./frontend\"",
    "watch-backend": "nodemon --watch ./backend --exec \"npm start --prefix ./backend\"",
    "start": "npm-run-all --parallel watch-frontend watch-backend",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "eslint": "^6.5.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5"
  }
}
